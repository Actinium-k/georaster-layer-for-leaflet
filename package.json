{
  "name": "georaster-layer-for-leaflet",
  "version": "0.7.1",
  "description": "Display GeoTIFFs and soon other types of raster on your Leaflet Map",
  "source": "src/georaster-layer-for-leaflet.ts",
  "main": "dist/georaster-layer-for-leaflet.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/georaster-layer-for-leaflet.min.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:babel": "babel src/georaster-layer-for-leaflet.ts -o dist/georaster-layer-for-leaflet.bundle.js",
    "build:browserify": "npm run build:babel && browserify dist/georaster-layer-for-leaflet.bundle.js -o dist/georaster-layer-for-leaflet.browserify.min.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:webpack": "webpack --config=webpack.config.js",
    "build": "npm run build:babel && npm run build:webpack && build:tsc",
    "dev": "concurrently \"npm run serve\" ",
    "format": "prettier --write georaster-layer-for-leaflet.js webpack.config.js",
    "fix": "eslint georaster-layer-for-leaflet.js --fix",
    "lint": "eslint georaster-layer-for-leaflet.js",
    "serve": "HOST=0.0.0.0 http-server --cors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeoTIFF/georaster-layer-for-leaflet.git"
  },
  "keywords": [
    "geotiff",
    "georaster",
    "gis",
    "leaflet",
    "maps",
    "raster",
    "rasters",
    "geoprocessing",
    "pixels",
    "tiff"
  ],
  "author": "Daniel Dufour and Steve Peyton",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GeoTIFF/georaster-layer-for-leaflet/issues"
  },
  "homepage": "https://github.com/GeoTIFF/georaster-layer-for-leaflet#readme",
  "dependencies": {
    "chroma-js": "^1.4.0",
    "regenerator-runtime": "^0.13.5",
    "utm-utils": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-template-literals": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.12.13",
    "@types/chroma-js": "^2.1.3",
    "@types/leaflet": "^1.5.21",
    "@types/node": "^14.14.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "^8.1.0",
    "browserify": "^14.5.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "http-server": "^0.12.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  },
  "peerDependencies": {
    "georaster": "^1.1.0",
    "leaflet": "^1.7.1"
  }
}
